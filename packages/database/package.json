{
  "name": "@renderer-orchestrator/database",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./orm": {
      "types": "./dist/orm/index.d.ts",
      "default": "./dist/orm/index.js"
    },
    "./prisma": {
      "types": "./dist/prisma/index.d.ts",
      "default": "./dist/prisma/index.js"
    },
    "./factory": {
      "types": "./dist/factory/create-prisma-pg-client.d.ts",
      "default": "./dist/factory/create-prisma-pg-client.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "gen": "node scripts/manage-generation.mjs",
    "gen:entry": "node scripts/prisma.generate-client-entry.mjs",
    "db:generate": "pnpm run gen:entry",
    "//": "generated 파일 생성",
    "postinstall": "pnpm run db:generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.3.0",
    "@prisma/client": "7.3.0",
    "@prisma/extension-read-replicas": "^0.5.0"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "prisma": "7.3.0"
  }
}
